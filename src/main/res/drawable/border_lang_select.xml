            <stroke
                android:width="@dimen/topwhite"
                android:color="#feeaa8" />
            <gradient
                android:centerColor="#fed244"
                android:endColor="#fdc81b"
                android:startColor="#fdc818" />

        </shape>
    </item>

</layer-list>
